{
  "name": "@kraken/cli",
  "description": "CLI pour les applications Kraken",
  "version": "1.0.0",
  "type": "module",
  "types": "types/index.d.ts",
  "main": "dist/index.js",
  "bin": {
    "kn": "dist/index.js"
  },
  "scripts": {
    "dev": "npm run clean:dist && npm run templates:build && tsc --watch",
    "build": "npm run clean:dist && npm run templates:build && tsc",
    "api:watch": "nodemon",
    "templates:build": "shx cp -r src/templates dist/templates",
    "clean:dist": "shx rm -rf dist && shx mkdir -p dist"
  },
