    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "find-in-files": "^0.5.0",
    "fs-extra": "^11.1.1",
    "get-caller-file": "^2.0.5",
    "glob": "^10.3.1",
    "inquirer": "^9.2.7",
    "inquirer-fuzzy-path": "^2.3.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
